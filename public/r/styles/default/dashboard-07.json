lectContent>\r\n                        </Select>\r\n                      </div>\r\n                    </div>\r\n                  </CardContent>\r\n                </Card>\r\n                <Card\r\n                  className=\"overflow-hidden\"\r\n                  x-chunk=\"A card with a form to upload product images\"\r\n                >\r\n                  <CardHeader>\r\n                    <CardTitle>Product Images</CardTitle>\r\n                    <CardDescription>\r\n                      Lipsum dolor sit amet, consectetur adipiscing elit\r\n                    </CardDescription>\r\n                  </CardHeader>\r\n                  <CardContent>\r\n                    <div className=\"grid gap-2\">\r\n                      <Image\r\n                        alt=\"Product image\"\r\n                        className=\"aspect-square w-full rounded-md object-cover\"\r\n                        height=\"300\"\r\n                        src=\"/placeholder.svg\"\r\n                        width=\"300\"\r\n                      />\r\n                      <div className=\"grid grid-cols-3 gap-2\">\r\n                        <button>\r\n                          <Image\r\n                            alt=\"Product image\"\r\n                            className=\"aspect-square w-full rounded-md object-cover\"\r\n                            height=\"84\"\r\n                            src=\"/placeholder.svg\"\r\n                            width=\"84\"\r\n                          />\r\n                        </button>\r\n                        <button>\r\n                          <Image\r\n                            alt=\"Product image\"\r\n                            className=\"aspect-square w-full rounded-md object-cover\"\r\n                            height=\"84\"\r\n                            src=\"/placeholder.svg\"\r\n                            width=\"84\"\r\n                          />\r\n                        </button>\r\n              